--ATUALIZAR NIVEL DE COMPATIBILIDADE POR BANCO

--VERIFICANDO VERS√O SQL
SELECT SERVERPROPERTY('ProductVersion')

--LOCALIZANDO VERS√O DE COMPATIBILIDADE DO BANCO
SELECT name, compatibility_level FROM sys.databases;

--APLICANDO ATUALIZA«√O NIVEL DE COMPATIBILIDADE POR BANCO
ALTER DATABASE #NomeDatabase  --INFORMAR NOME DO BANCO 
SET COMPATIBILITY_LEVEL = 110;  --INFORMAR GRAU DE COMPATIBILIDADE CONFORME VERS√O DO SQLSERVER
GO

